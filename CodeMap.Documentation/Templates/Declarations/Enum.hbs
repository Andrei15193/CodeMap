{{#with declarationNode}}
<h2>{{name}} Enum</h2>

{{DocumentationContent summary}}

<h3>Members</h3>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Name</th>
            <th>Value</th>
            <th class="w-75">Summary</th>
        </tr>
    </thead>
    <tbody>
        {{#each members}}
        <tr>
            <td>{{name}}</td>
            <td>{{Format value 'D'}}</td>
            <td>{{DocumentationContent summary}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>

{{DocumentationContent remarks}}
{{/with}}